<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lance.rxjavatest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tools"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.activity.MainActivity"
            android:label="主程序">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.WeatherActivity"
            android:label="天气查询"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.MobileAttributionActivity"
            android:label="手机号归属地查询"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.IDCardInquiryActivity"
            android:label="身份证查询"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.activity.IPAddressActivity"
            android:label="ip地址查询"
            android:screenOrientation="portrait" />
    </application>

</manifest>
